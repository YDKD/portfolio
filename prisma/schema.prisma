datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

generator client {
  provider = "prisma-client-js"
  output   = "./generated/client"
}

model Link {
  id         Int      @id @default(autoincrement())
  url        String   @unique
  title      String?
  ogImageUrl String?
  createdAt  DateTime @default(now())
}
